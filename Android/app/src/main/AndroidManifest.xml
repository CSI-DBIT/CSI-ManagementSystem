            android:name=".mActivityManager.Creative"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".mActivityManager.Proposal"
            android:theme="@style/AppTheme2" />
        <activity android:name=".Prompts.ProfileEdit"
            android:theme="@style/AppTheme2"/>
        <activity
            android:name=".Prompts.Manager"
            android:label="@string/title_activity_manager"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Prompts.role_intent" />
        <activity android:name=".Prompts.AddMinute" />
        <activity
            android:name=".Prompts.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>