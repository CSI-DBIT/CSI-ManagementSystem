<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.csi">

    <application
        android:allowBackup="true"
        android:icon="@drawable/csilogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/csilogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity android:name=".mActivityManager.publcity_recycler" />
        <activity android:name=".Prompts.attendance_list"
            android:theme="@style/AppTheme2"/>
        <activity
            android:name=".mActivityManager.edit_proposal"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".mActivityManager.proposal_desc"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".mActivityManager.praposal_recycler"
            android:theme="@style/AppTheme2" />
        <activity android:name=".Gallery.Activities.FullScreenActivity" />
        <activity
            android:name=".Gallery.Activities.DisplayImage"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".Gallery.Activities.DisplayYear"
            android:theme="@style/AppTheme2" />
        <activity
            android:name=".Gallery.Activities.DisplayEventName"
            android:theme="@style/AppTheme2" />
        <activity android:name=".mActivityManager.Technical_form" />
        <activity
            android:name=".mActivityManager.Creative_form"
